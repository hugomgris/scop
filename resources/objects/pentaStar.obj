# Complex 2D Star with Pentagon Center
# This creates a 5-pointed star with a pentagon in the center
o ComplexStar

# Pentagon center vertices (inner)
v  0.0    0.2   0.0   # 1 - top
v  0.19   0.062 0.0   # 2 - top right
v  0.118 -0.162 0.0   # 3 - bottom right
v -0.118 -0.162 0.0   # 4 - bottom left
v -0.19   0.062 0.0   # 5 - top left

# Star outer points
v  0.0    0.8   0.0   # 6 - top point
v  0.76   0.248 0.0   # 7 - top right point
v  0.472 -0.648 0.0   # 8 - bottom right point
v -0.472 -0.648 0.0   # 9 - bottom left point
v -0.76   0.248 0.0   # 10 - top left point

# Star inner valleys (between outer points)
v  0.35   0.35  0.0   # 11 - between top and top-right
v  0.35  -0.15  0.0   # 12 - between top-right and bottom-right
v  0.0   -0.35  0.0   # 13 - between bottom-right and bottom-left
v -0.35  -0.15  0.0   # 14 - between bottom-left and top-left
v -0.35   0.35  0.0   # 15 - between top-left and top

# Texture coordinates
vt 0.5  0.6   # 1 - center pentagon
vt 0.595 0.531
vt 0.559 0.419
vt 0.441 0.419
vt 0.405 0.531

vt 0.5  0.9   # 6 - star points
vt 0.88 0.624
vt 0.736 0.176
vt 0.264 0.176
vt 0.12  0.624

vt 0.675 0.675 # 11 - star valleys
vt 0.675 0.425
vt 0.5   0.325
vt 0.325 0.425
vt 0.325 0.675

# Normals (all facing forward in 2D)
vn 0.0 0.0 1.0

# Pentagon center faces
f 1/1/1 2/2/1 11/11/1
f 2/2/1 3/3/1 12/12/1
f 3/3/1 4/4/1 13/13/1
f 4/4/1 5/5/1 14/14/1
f 5/5/1 1/1/1 15/15/1

# Star point triangles
f 1/1/1 11/11/1 6/6/1
f 2/2/1 12/12/1 7/7/1
f 3/3/1 13/13/1 8/8/1
f 4/4/1 14/14/1 9/9/1
f 5/5/1 15/15/1 10/10/1

# Star valley triangles (connecting points)
f 11/11/1 2/2/1 7/7/1
f 12/12/1 3/3/1 8/8/1
f 13/13/1 4/4/1 9/9/1
f 14/14/1 5/5/1 10/10/1
f 15/15/1 1/1/1 6/6/1

# Additional inner triangles to fill gaps
f 11/11/1 12/12/1 2/2/1
f 12/12/1 13/13/1 3/3/1
f 13/13/1 14/14/1 4/4/1
f 14/14/1 15/15/1 5/5/1
f 15/15/1 11/11/1 1/1/1